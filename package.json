{
  "name": "jinni",
  "version": "3.6.2",
  "description": "A boilerplate generator for front end projects",
  "type": "module",
  "engines": {
    "node": "14"
  },
  "keywords": [
    "front",
    "end",
    "stack",
    "boilerplate",
    "generator",
    "jinni"
  ],
  "license": "GPL-3.0",
  "author": "LeKangouroo",
  "repository": {
    "type": "git",
    "url": "git+https://LeKangouroo@github.com/LeKangouroo/jinni.git"
  },
  "bugs": {
    "url": "https://github.com/LeKangouroo/jinni/issues"
  },
  "homepage": "https://github.com/LeKangouroo/jinni#readme",
  "bin": {
    "jin": "./src/bin/cli.js"
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  },
  "scripts": {
    "release": "standard-version",
    "sandbox": "rm -r _sandbox ; npm run setup && mkdir _sandbox && cd _sandbox && jin init",
    "setup": "npm install -g",
    "test": "mocha --recursive tests"
  },
  "dependencies": {
    "canvas": "2.6.1",
    "chalk": "4.1.0",
    "css-mediaquery": "0.1.2",
    "fancy-log": "1.3.3",
    "fs-extra": "9.0.1",
    "inquirer": "7.3.3",
    "jsdom": "16.4.0",
    "mocha": "8.1.3",
    "ramda": "0.27.1",
    "rlite-router": "2.0.3",
    "standard-version": "9.0.0",
    "urijs": "1.19.2",
    "validate-npm-package-name": "3.0.0",
    "yargs": "15.4.1"
  }
}
