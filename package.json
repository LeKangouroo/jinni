{
  "name": "jinni",
  "version": "3.5.1",
  "description": "A boilerplate generator for front end projects",
  "engines": {
    "node": "10"
  },
  "keywords": [
    "front",
    "end",
    "stack",
    "boilerplate",
    "generator",
    "jinni"
  ],
  "license": "MIT",
  "author": "Itadakimas",
  "repository": {
    "type": "git",
    "url": "git+https://itadakimas@github.com/itadakimas/jinni.git"
  },
  "bugs": {
    "url": "https://github.com/itadakimas/jinni/issues"
  },
  "homepage": "https://github.com/itadakimas/jinni#readme",
  "bin": {
    "jin": "./src/bin/cli.js"
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  },
  "scripts": {
    "release": "standard-version",
    "sandbox": "rm -r _sandbox ; npm run setup && mkdir _sandbox && cd _sandbox && jin init",
    "setup": "npm install -g",
    "test": "mocha --recursive --require babel-register tests"
  },
  "dependencies": {
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "chalk": "2.4.1",
    "core-js": "2.5.7",
    "css-mediaquery": "0.1.2",
    "fancy-log": "1.3.2",
    "fs-extra": "7.0.0",
    "inquirer": "6.2.0",
    "jsdom": "13.0.0",
    "lodash": "4.17.11",
    "mocha": "5.2.0",
    "rlite-router": "2.0.3",
    "standard-version": "4.4.0",
    "urijs": "1.19.1",
    "validate-npm-package-name": "3.0.0",
    "yargs": "12.0.2"
  }
}
