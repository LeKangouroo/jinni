{
  "name": "jinni",
  "version": "3.5.1",
  "description": "A boilerplate generator for front end projects",
  "engines": {
    "node": "8"
  },
  "keywords": [
    "front",
    "end",
    "stack",
    "boilerplate",
    "generator",
    "jinni"
  ],
  "license": "MIT",
  "author": "Itadakimas",
  "repository": {
    "type": "git",
    "url": "git+https://itadakimas@github.com/itadakimas/jinni.git"
  },
  "bugs": {
    "url": "https://github.com/itadakimas/jinni/issues"
  },
  "homepage": "https://github.com/itadakimas/jinni#readme",
  "bin": {
    "jin": "./src/bin/cli.js"
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  },
  "scripts": {
    "release": "standard-version",
    "setup": "npm install -g",
    "test": "mocha --recursive --compilers js:babel-register tests"
  },
  "dependencies": {
    "babel-preset-env": "1.6.0",
    "babel-register": "6.26.0",
    "chalk": "1.1.3",
    "core-js": "2.5.0",
    "css-mediaquery": "0.1.2",
    "fs-extra": "5.0.0",
    "inquirer": "1.2.1",
    "jsdom": "11.2.0",
    "lodash": "4.17.5",
    "mocha": "3.5.0",
    "rlite-router": "2.0.3",
    "standard-version": "4.2.0",
    "urijs": "1.18.4",
    "validate-npm-package-name": "2.2.2",
    "yargs": "6.6.0"
  }
}
